{
  "name": "sensing-flow-mobile",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:android": "dotenvx run -f .env.development -- tauri android dev",
    "dev:ios": "dotenvx run -f .env.development -- tauri ios dev",
    "dev:ios:16": "dotenvx run -f .env.development -- tauri ios dev \"iPhone 16\"",
    "dev:ios:open": "dotenvx run -f .env.development -- tauri ios dev --open",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "tauri": "dotenvx run -f .env.development -- tauri",
    "lint": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "check": "prettier --write . && eslint --fix",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "gen:api": "curl http://localhost:5173/docs-json > swagger.json && openapi-typescript ./swagger.json -o src/api.types.gen.ts"
  },
  "dependencies": {
    "@base-ui/react": "^1.1.0",
    "@storybook/react-vite": "^10.1.11",
    "@tabler/icons-react": "^3.36.1",
    "@tanstack/react-devtools": "^0.9.2",
    "@tanstack/react-query": "^5.90.16",
    "@tanstack/react-query-devtools": "^5.91.2",
    "@tanstack/react-router": "^1.147.0",
    "@tanstack/react-router-devtools": "^1.147.0",
    "@tanstack/react-store": "^0.8.0",
    "@tanstack/router-plugin": "^1.147.0",
    "@tanstack/store": "^0.8.0",
    "@tanstack/zod-adapter": "^1.150.0",
    "@tauri-apps/api": "^2.9.1",
    "@tauri-apps/plugin-deep-link": "~2",
    "@tauri-apps/plugin-opener": "^2.5.3",
    "@tauri-apps/plugin-os": "^2.3.2",
    "@tauri-apps/plugin-stronghold": "~2",
    "classnames": "^2.5.1",
    "framer-motion": "^12.26.2",
    "i18next": "^25.7.4",
    "openapi-fetch": "^0.15.0",
    "openapi-typescript": "^7.10.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-i18next": "^16.5.2",
    "storybook": "^10.1.11",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.3",
    "@eslint/js": "^9.39.2",
    "@satooru65536/tauri-plugin-auth-cognito": "./tauri-plugin-auth-cognito",
    "@satooru65536/tauri-plugin-sensorkit": "./tauri-plugin-sensorkit",
    "@tanstack/devtools-event-client": "^0.4.0",
    "@tanstack/devtools-vite": "^0.4.1",
    "@tanstack/eslint-config": "^0.3.4",
    "@tanstack/router-vite-plugin": "^1.150.0",
    "@tauri-apps/cli": "^2",
    "@types/node": "^25.0.5",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^5.1.2",
    "babel-plugin-react-compiler": "^1.0.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "globals": "^17.0.0",
    "jsdom": "^27.4.0",
    "prettier": "^3.7.4",
    "sass": "^1.97.2",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.52.0",
    "vite": "^7.0.4",
    "vitest": "^4.0.16"
  }
}
